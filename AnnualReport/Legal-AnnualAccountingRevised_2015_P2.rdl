    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>="SELECT
  TO_CHAR(WARD_NUMBER) WARD_NUMBER,
  WARD_NAME,
  WARD_COURT_FILE_NO,
  WARD_DIVISION,
  TO_CHAR(ANNUAL_FEE_WAIVER_GRANTED,'MM/DD/YYYY') ANNUAL_FEE_WAIVER_GRANTED,  
  (
    SELECT
      get_scheduleA_bal(WARD_NUMBER ,'"+ Parameters!StartDate.Value +"'    )    + 
      get_scheduleB_bal_ALL(WARD_NUMBER,'"+ Parameters!StartDate.Value +"') + 
      get_scheduleC_bal(WARD_NUMBER,'"+ Parameters!StartDate.Value +"')     + 
      get_scheduleD_bal(WARD_NUMBER,'"+ Parameters!StartDate.Value +"')     + 
      (SELECT sum(SCHEDULEE_AMOUNT) FROM SCHEDULEE
where SCHEDULEE_WARD = "+ Parameters!Ward.Value +"  and SCHEDULEE_TRANS_DATE &lt; to_date('"+ Parameters!StartDate.Value +"','mm/dd/yyyy'))
    FROM
      dual
  )
  LY_BBF ,  
  (

SELECT  
sum(receipt) receipts
FROM
  (
    SELECT
      SCHEDULEA_TRANS_DATE,
      DECODE(SCHEDULEA_TRANS_TYPE,'R','DEPOSIT'
      ||'-'
      ||SCHEDULEA_DESCRIPTION, VENDOR_NAME
      ||'-'
      ||SCHEDULEA_DESCRIPTION) SCHEDULEA_DESCRIPTION,
      TO_NUMBER(DECODE(SCHEDULEA_TRANS_TYPE,'R','',SCHEDULEA_AMOUNT * -1))
      DISBUR,
      TO_NUMBER(DECODE(SCHEDULEA_TRANS_TYPE,'D','',SCHEDULEA_AMOUNT)) RECEIPT 
       
    FROM
      SCHEDULEA,
      WARD,
      VENDOR
    WHERE
      WARD_NUMBER      ="+ Parameters!Ward.Value +"  
    AND SCHEDULEA_WARD = WARD_NUMBER
    AND TRUNC(SCHEDULEA_TRANS_DATE) BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY')
      AND TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
     AND schedulea_trans_type = 'R'
    AND SCHEDULEA_VENDOR = VENDOR_NUMBER(+)
    UNION ALL
    SELECT
      SCHEDULEB_DATE SCHEDULEA_TRANS_DATE,
      scheduleb.scheduleb_desc,
       TO_NUMBER(DECODE(SCHEDULEB_TYPE,'D','',SCHEDULEB_AMOUNT * -1))
      DISBUR,
      TO_NUMBER(DECODE(SCHEDULEB_TYPE,'W','',SCHEDULEB_AMOUNT)) RECEIPT 
      
    FROM
       SCHEDULEB
    WHERE
      scheduleb.scheduleb_ward             ="+ Parameters!Ward.Value +"     
    AND SCHEDULEB_DATE  BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY') AND
      TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )   WHERE  receipt &gt; 0   



  ) SCHEDA_IN,
  (
  (
    SELECT
      NVL(SUM(SCHEDULEA_AMOUNT),0)
    FROM
      SCHEDULEA
    WHERE
       
    SCHEDULEA_WARD = "+ Parameters!Ward.Value +"  --WARD_NUMBER
    AND TRUNC(SCHEDULEA_TRANS_DATE) BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY')
      AND TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
     AND schedulea_trans_type = 'D'
  )
  +
  (
    SELECT
      NVL(SUM(SCHEDULEb_AMOUNT),0)
    FROM
      scheduleb
    WHERE
      scheduleb_ward   = WARD_NUMBER
    AND scheduleb_type = 'W'
    AND TRUNC(scheduleb_date) BETWEEN to_date('"+ Parameters!StartDate.Value +"','mm/dd/yyyy') AND
      TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  ) SCHEDA_OUT,
  (
    SELECT
      NVL(SUM(SCHEDULEA_AMOUNT),0)
    FROM
      SCHEDULEA
    WHERE
      SCHEDULEA_WARD    = WARD_NUMBER
    AND SCHEDULEA_TRANS_DATE &lt;= TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  SCHEDA_BAL,
  (
    SELECT
      NVL(SUM(SCHEDULEB_AMOUNT),0)
    FROM
      SCHEDULEB
    WHERE
      SCHEDULEB_WARD    = WARD_NUMBER
    AND SCHEDULEB_DATE &lt;= TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  SCHEDB_BAL,
  (
    SELECT
      NVL(SUM(SCHEDULEC_AMOUNT),0)
    FROM
      SCHEDULEC
    WHERE
      SCHEDULEC_WARD          = WARD_NUMBER
    AND SCHEDULEC_TRANS_DATE &lt;= TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  SCHEDC_BAL,
  (
    SELECT
      NVL(SUM(SCHEDULED_AMOUNT),0)
    FROM
      SCHEDULED
    WHERE
      SCHEDULED_WARD          = WARD_NUMBER
    AND SCHEDULED_TRANS_DATE &lt;= TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  SCHEDD_BAL,
  (
    SELECT
      NVL(SUM(SCHEDULEE_AMOUNT),0)
    FROM
      SCHEDULEE
    WHERE
      SCHEDULEE_WARD          = WARD_NUMBER
    AND SCHEDULEE_TRANS_DATE &lt;= TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
  )
  SCHEDE_BAL,
  TO_CHAR(WARD_START_STATUS_DATE,'MM/DD/YYYY') WARD_START_STATUS_DATE,
  '"+ Parameters!StartDate.Value +"'  BILLSEL_BEG_ANNUAL_DATE,
'"+ Parameters!EndDate.Value +"'  BILLSEL_END_ANNUAL_DATE,
  flip_name(VENDOR_NAME) VENDOR_NAME,
  VENDOR_ADDRESS_1,
  VENDOR_ADDRESS_2,
  VENDOR_ADDRESS_3,
  VENDOR_TELEPHONE,
  VENDOR_CONTACT,
(select sum(gain + loss )  
from
(SELECT
  DECODE(SIGN(SCHEDULEC_AMOUNT),1, SCHEDULEC_AMOUNT,-1,0) gain ,
  DECODE(SIGN(SCHEDULEC_AMOUNT),                    -1, SCHEDULEC_AMOUNT,1,0)
  loss
FROM
  schedulec c,
  ward w,
  schedulecid cid
WHERE
  1                   = 1
AND SCHEDULEC_AMOUNT !=0
AND schedulec_ward    = "+ Parameters!Ward.Value +"  
AND c.schedulec_ward  = cid.schedulecid_ward
AND W.WARD_NUMBER     = c.schedulec_ward
AND c.schedulec_id    = cid.schedulecid_id_number
AND TRUNC(c.schedulec_trans_date) BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY') AND
  TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
UNION ALL
SELECT
  DECODE(SIGN(SCHEDULEd_AMOUNT),1, SCHEDULEd_AMOUNT,-1,0) gain,
  DECODE(SIGN(SCHEDULEd_AMOUNT),                    -1, SCHEDULEd_AMOUNT,1,0)
  loss
FROM
  scheduled d,
  WARD W,
  scheduledid did
WHERE
  1                   = 1
AND SCHEDULED_AMOUNT !=0
AND scheduled_ward    = "+ Parameters!Ward.Value +"  
AND d.scheduled_ward  = did.scheduledid_ward
AND d.scheduled_id    = did.scheduledid_id_number
AND W.WARD_NUMBER     = d.scheduled_ward
AND TRUNC(d.scheduled_trans_date) BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY') AND
  TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY')
UNION ALL
SELECT
  DECODE(SIGN(SCHEDULEE_AMOUNT),1, SCHEDULEE_AMOUNT,-1,0) gain,
  DECODE(SIGN(SCHEDULEE_AMOUNT),                    -1, SCHEDULEE_AMOUNT,1,0)
  loss
FROM
  schedulee e ,
  WARD W,
  scheduleeid ecid
WHERE
  1                   =1
AND SCHEDULEE_AMOUNT !=0
AND e.schedulee_ward  = "+ Parameters!Ward.Value +"  
AND e.schedulee_ward  = ecid.scheduleeid_ward
AND W.WARD_NUMBER     = E.SCHEDULEE_WARD
AND TRUNC(e.schedulee_trans_date) BETWEEN TO_DATE('"+ Parameters!StartDate.Value +"','MM/DD/YYYY') AND
  TO_DATE('"+ Parameters!EndDate.Value +"','MM/DD/YYYY') )) GAIN,NVL(GET_ASSET_MARKET_VALUE("+ Parameters!Ward.Value +"  ,'"+ Parameters!EndDate.Value +"') + GET_SCHEDULEA_BAL("+ Parameters!Ward.Value +"  ,'"+ Parameters!EndDate.Value +"') + GET_SCHEDULEB_BAL_ALL("+ Parameters!Ward.Value +"  ,'"+ Parameters!EndDate.Value +"'),0) CurrentVal
FROM
  WARD,
  VENDOR,
  ANNUAL_FEE_WAIVER 
  
WHERE
  WARD_NUMBER                IN ("+ Parameters!Ward.Value +"  )
AND WARD_GUARDIAN_TYPE       &lt;&gt; 0
AND VENDOR_NUMBER(+)          = WARD_ATTORNEY_NUMBER
AND ANNUAL_FEE_WAIVER_WARD(+) = WARD_NUMBER"</CommandText>
        <QueryParameters>
